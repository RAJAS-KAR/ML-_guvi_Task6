      "source": [
        "def combinations(L):\n",
        "  if L:\n",
        "    result = combinations(L[:-1])\n",
        "    return result+[i+[L[-1]]for i in result]\n",
        "  else:\n",
        "    return[[]]\n",
        "combi=combinations(['Mileage','Cylinder','Liter', 'Doors', 'Cruise', 'Sound', 'Leather'])\n",
        "combi = combi[1:]\n",
        "print(combi)"
